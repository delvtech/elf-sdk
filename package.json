{
    "name": "elf-sdk",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "npx hardhat compile && rm ./typechain/index.ts",
        "test": "npx hardhat compile && rm ./typechain/index.ts && npx hardhat test --config hardhat.config.test.ts ./test/*.ts",
        "style-check": "npx prettier --check .",
        "style-fix": "npx prettier --write .",
        "lint-check": "eslint .",
        "lint-fix": "eslint --fix .",
        "generate-interfaces": "npx hardhat run scripts/generate-interfaces.ts",
        "load-contracts": "sh scripts/load-element.sh && sh scripts/load-balancer.sh"
    },
    "_moduleAliases": {
        "test": "./test",
        "src": "./src"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.1",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@typechain/ethers-v5": "^5.0.0",
        "@types/chai-almost": "^1.0.1",
        "@types/mocha": "^8.2.0",
        "@types/node": "^10.17.60",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "chai": "^4.2.0",
        "chai-almost": "^1.0.1",
        "eslint": "^7.28.0",
        "ethereum-waffle": "^3.2.2",
        "ethers": "^5.0.29",
        "hardhat": "^2.1.2",
        "hardhat-gas-reporter": "^1.0.4",
        "hardhat-typechain": "^0.3.4",
        "json-to-ts": "^1.7.0",
        "prettier": "^2.2.1",
        "ts-generator": "^0.1.1",
        "ts-node": "^9.1.1",
        "typechain": "^4.0.1",
        "typescript": "^4.1.3"
    }
}
